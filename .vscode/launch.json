{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Debug cf_search.py",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/src/cf_search.py",
      "console": "integratedTerminal",
      "justMyCode": false,
      "args": [
        "--dataset",
        "cif2016",
        "--horizon",
        "12",
        "--back-horizon",
        "15",
        "--split-size",
        "0.6",
        "0.2",
        "0.2",
        "--stride-size",
        "1",
        "--center",
        "median",
        "--desired-shift",
        "0",
        "--desired-change",
        "0.1",
        "--poly-order",
        "1",
        "--fraction-std",
        "1",
        "--ablation-horizon",
        "12",
        "--random-seed",
        "1", // Note: $seed was replaced with a specific value, change as needed.
        "--output",
        "output.csv"
      ]
    }
  ]
}